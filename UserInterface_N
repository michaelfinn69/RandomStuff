k={key=Z.key or"";value=Z.value or"";order=Z.order or#P.values+1,enabled=Z.enabled==nil and true or Z.enabled,objects={}}table.insert(P.values,k)do local Z=q.zindexOrder.indicator local P=k.objects P.background=r:Draw("Square",{Size=S(1,0,0,16);ThemeColor="Background",ZIndex=Z,Parent=w.objects.background})P.border1=r:Draw("Square",{Size=S(1,2,1,2);Position=S(0,-1,0,-1),ThemeColor="Border 2",Parent=P.background;ZIndex=Z-1})P.border2=r:Draw("Square",{Size=S(1,2,1,2),Position=S(0,-1,0,-1),ThemeColor="Border 3",Parent=P.border1;ZIndex=Z-2})P.keyLabel=r:Draw("Text",{Position=S(0,3,0,1);ThemeColor="Option Text 2";Size=13;Font=2;ZIndex=Z+2;Outline=true;Parent=P.background})P.valueLabel=r:Draw("Text",{Position=S(0,0,0,1),ThemeColor="Option Text 2",Size=13;Font=2;ZIndex=Z+2;Outline=true,Parent=P.background})end function k.Remove(Z)table.remove(w.values,table.find(w.values,k))Z.objects.background:Remove()table.clear(Z)w:Update()end function k.SetEnabled(P,Z)if typeof(Z)=="boolean"then P.enabled=Z w:Update()end end function k.SetValue(P,Z)if typeof(Z)=="string"then P.value=Z w:Update()end end function k.SetKey(P,Z)if typeof(Z)=="string"then P.key=Z w:Update()end end P:Update()return k end function w.GetValue(P,Z)if typeof(Z)=="number"then return P.values[Z]else for P,w in m,P.values do if w.key==Z then return w end end end end function w.SetEnabled(P,Z)if typeof(Z)=="boolean"then P.enabled=Z P.objects.background.Visible=Z P:Update()end end function w.SetPosition(P,Z)if typeof(Z)=="UDim2"then P.position=Z P.objects.background.Position=Z end end for Z,P in m,P.values or{}do w:AddValue({key=tostring(Z),value=tostring(P)})end w:SetEnabled(w.enabled)return w end function Z.NewWindow(Z)local w={title=Z.title or"",selectedTab=nil,tabs={};objects={},colorpicker={objects={},color=j(1,0,0);trans=0};dropdown={objects={values={}};max=5}}table.insert(q.windows,w)do local P=Z.size or S(0,525,0,650)local k=Z.position or S(0,250,0,150)local T=w.objects local f=q.zindexOrder.window T.backgroun