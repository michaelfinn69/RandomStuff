eturn J end function D.UpdateSections(Z)table.sort(Z.sections,function(Z,P)return Z.order<P.order end)local P,w local k=15 for Z,T in m,Z.sections do if T.objects.background.Visible~=(T.enabled and D.selected)then T.objects.background.Visible=T.enabled and D.selected T:UpdateOptions()end if T.enabled then if T.side==1 then if P then T.objects.background.Position=P.objects.background.Position+S(0,0,0,P.objects.background.Object.Size.Y+k)end P=T elseif T.side==2 then if w then T.objects.background.Position=w.objects.background.Position+S(0,0,0,w.objects.background.Object.Size.Y+k)end w=T end end T:SetText(T.text)end end function D.SetText(P,Z)if typeof(Z)=="string"then P.text=Z P.objects.text.Text=Z w:UpdateTabs()end end function D.Select(Z)w.selectedTab=D w:UpdateTabs()for Z,P in m,w.tabs do if P.callback then P.callback(P==D)end end end if w.selectedTab==nil then D:Select()end D:SetText(D.text)w:UpdateTabs()return D end function w.UpdateTabs(Z)table.sort(Z.tabs,function(Z,P)return Z.order<P.order end)local P=0 for w,k in m,Z.tabs do local T=k.objects k.selected=k==Z.selectedTab T.background.ThemeColor=k.selected and"Selected Tab Background"or"Unselected Tab Background"T.background.Size=S(0,T.text.TextBounds.X+14,1,k.selected and 1 or 0)T.background.Position=S(0,P,0,0)T.text.ThemeColor=k.selected and"Selected Tab Text"or"Unselected Tab Text"T.text.Position=S(.5,0,0,3)T.topBorder.ThemeColor=k.selected and"Accent"or"Unselected Tab Background"P+=T.background.Size.X.Offset+1 k:UpdateSections()end end w:SetOpen(true)return w end do local Z=q.zindexOrder.window+2000 P.background=r:Draw("Square",{ThemeColor="Group Background",ZIndex=Z;Visible=false})P.border1=r:Draw("Square",{Size=UDim2.new(1,2,1,2);Position=UDim2.new(0,-1,0,-1);ThemeColor="Border 1";ZIndex=Z-1;Parent=P.background})P.border2=r:Draw("Square",{Size=UDim2.new(1,4,1,4),Position=UDim2.new(0,-2,0,-2),ThemeColor="Border 3",ZIndex=Z-2;Parent=P.background})P.text=r:Draw("Text",{Position=UDim2.new(0,3,0,0),ThemeColor