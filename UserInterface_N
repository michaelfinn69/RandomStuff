or="Accent",ZIndex=w+1,Parent=P.background})P.text=r:Draw("Text",{Position=S(.5,0,0,2);ThemeColor="Primary Text",Text="Watermark Text",Size=13,Font=2,ZIndex=w+1,Outline=true;Center=true;Parent=P.background})end end local J=tick()r:Connection(T.RenderStepped,function(Z)q.stats.fps=C(1/Z)q.stats.ping=D.Network.ServerStatsItem["Data Ping"]:GetValue()q.stats.sendkbps=D.DataSendKbps q.stats.receivekbps=D.DataReceiveKbps if(tick()-J)*1000>q.watermark.refreshrate then J=tick()q.watermark:Update()end end)Z.keyIndicator=Z.NewIndicator({title="Keybinds";pos=S(0,15,0,325),enabled=false})Z:SetTheme(q.themes[1].theme)Z:SetOpen(true)Z.hasInit=true end function q.CreateSettingsTab(P,Z)local w=Z:AddTab("Settings",999)local T=w:AddSection("Config",2)local f=w:AddSection("Main",1)T:AddBox({text="Config Name",flag="configinput"})T:AddList({text="Config";flag="selectedconfig"})local function z()q.options.selectedconfig:ClearValues()for Z,w in m,listfiles(P.cheatname..("/"..(P.gamename.."/configs")))do local k="."..(w:split("."))[#w:split(".")]if k==P.fileext then q.options.selectedconfig:AddValue((w:split("\\"))[#w:split("\\")]:sub(1,-(#k)-1))end end end;(T:AddButton({text="Load",confirm=true;callback=function()q:LoadConfig(q.flags.selectedconfig)end})):AddButton({text="Save",confirm=true,callback=function()q:SaveConfig(q.flags.selectedconfig)end});(T:AddButton({text="Create";confirm=true,callback=function()if q:GetConfig(q.flags.configinput)then q:SendNotification("Config \'"..(q.flags.configinput.."\' already exists."),5,j(1,0,0))return end writefile(P.cheatname..("/"..(P.gamename..("/configs/"..(q.flags.configinput..P.fileext)))),k:JSONEncode({}))z()end})):AddButton({text="Delete",confirm=true,callback=function()if q:GetConfig(q.flags.selectedconfig)then delfile(P.cheatname..("/"..(P.gamename..("/configs/"..(q.flags.selectedconfig..P.fileext)))))z()end end})T:AddButton({text="Refresh",confirm=true,callback=z})z()f:AddBind({text="Open / Close";flag="togglebind";nomouse=true,noindicator=true,default=Enum.KeyCode.LeftAlt,bind=Enum.KeyCode.LeftAlt;callback=function()q:SetOpen(not q.open)end})f:AddToggle({text="Disable Movement If Open",flag="disablemenumovement";callback=function(Z)if Z and q.open then M:BindAction("FreezeMovement",function()return Enum.ContextActionResult.Sink end,false,unpack(Enum.PlayerActions:GetEnumItems()))else M:UnbindAction("FreezeMovement")end end})f:AddButton({text="Join Discord",flag="joindiscord",confirm=true,callback=function()local Z=syn.request({Url="http://127.0.0.1:6463/rpc?v=1",Method="POST";Headers={["Content-Type"]="application/json";Origin="https://discord.com"},Body=(game:GetService("HttpService")):JSONEncode({cmd="INVITE_BROWSER";nonce=(game:GetService("HttpService")):GenerateGUID(false),args={code="https://discord.gg/yb8fzNcM2E"}})})if Z.Success then q:SendNotification(q.cheatname.." | joined discord",3)end end})f:AddButton({text="Copy Discord",flag="copydiscord";callback=function()setclipboard("https://discord.gg/yb8fzNcM2E")end})f:AddButton({text="Copy Game Invite",callback=function()setclipboard("Roblox.GameLauncher.joinGameInstance("..(game.PlaceId..(",\""..(game.JobId.."\")"))))end})f:AddButton({text="Unload",confirm=true;callback=function()q:Unload()end})f:AddSeparator({text="Keybinds"})f:AddToggle({text="Keybind Indicator",flag="keybind_indicator";callback=function(Z)q.keyIndicator:SetEnabled(Z)end})f:AddSlider({text="Position X",flag="keybind_indicator_x",min=0,max=100;increment=.1,value=.5,callback=function()q.keyIndicator:SetPosition(S(q.flags.keybind_indicator_x/100,0,q.flags.keybind_indicator_y/100,0))end})f:AddSlider({text="Position Y",flag="keybind_indicator_y";min=0,max=100,increment=.1,value=35,callback=function()q.keyIndicator:SetPosition(S(q.flags.keybind_indicator_x/100,0,q.flags.keybind_indicator_y/100,0))end})f:AddSeparator({text="Watermark"})f:AddToggle({text="Enabled",flag="watermark_enabled"})f:AddList({text="Position";flag="watermark_pos";selected="Custom",values={"Top","Top Left";"Top Right";"Bottom Left","Bottom Right";"Custom"};callback=function(Z)q.watermark.lock=Z end})f:AddSlider({text="Custom X";flag="watermark_x";suffix="%";min=0;max=100;increment=.1})f:AddSlider({text="Custom Y",flag="watermark_y",suffix="%",min=0;max=100;increment=.1})local D={"Custom"}for Z,P in m,q.themes do table.insert(D,P.name)end local I=w:AddSection("Theme",2)I:AddColor({text="Accent";flag="theme_accent";default=x(255,135,255);callback=function(Z)q.theme.Accent=Z q:SetTheme(q.theme)end});(I:AddList({text="Presets";flag="preset_theme";values=D;callback=function(Z)for P,w in m,q.themes do if w.name==Z then q.options.theme_accent:SetColor(w.theme.Accent)q:SetTheme(w.theme)break end end end})):Select("Custom")return w end;(getgenv()).library=q return q
